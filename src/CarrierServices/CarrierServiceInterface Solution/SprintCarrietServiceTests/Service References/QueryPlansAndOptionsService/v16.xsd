<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://integration.sprint.com/interfaces/queryAvailableOptions/v1/queryAvailableOptions.xsd" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://integration.sprint.com/interfaces/queryAvailableOptions/v1/queryAvailableOptions.xsd" version="4.9" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:annotation>
    <xs:documentation>11/30/2009    Adriana Fernandes 2.1  Added  soc     GBL10.2                         PJ001210</xs:documentation>
  </xs:annotation>
  <xs:element name="queryAvailableOptions" type="tns:QueryAvailableOptionsType" />
  <xs:element name="queryAvailableOptionsResponse" type="tns:QueryAvailableOptionsResponseType" />
  <xs:complexType name="QueryAvailableOptionsType">
    <xs:sequence>
      <xs:element name="callingApplicationInfo" type="tns:CallingApplicationInfoType">
        <xs:annotation>
          <xs:documentation>Structure providing information regarding the calling front end</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice>
        <xs:element name="searchCriteria" type="tns:SearchCriteriaType">
          <xs:annotation>
            <xs:documentation>Structure providing the criteria in which to search by.</xs:documentation>
          </xs:annotation>
        </xs:element>
        <xs:element name="searchBySocList" type="tns:SearchBySocListType">
          <xs:annotation>
            <xs:documentation>Contains the list of search by soc information .</xs:documentation>
          </xs:annotation>
        </xs:element>
      </xs:choice>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="QueryAvailableOptionsResponseType">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="pricePlanInfo" type="tns:PricePlanInfoResponseType">
        <xs:annotation>
          <xs:documentation>Structure providing plan options detail info records.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="maxStackableSocIndicator" type="tns:BinaryExpressionType">
        <xs:annotation>
          <xs:documentation>Maximum Stackable SOC Indicator. If true, it means that the maximum limit allowed for the Subscriber has been reached. If false, then the Subscriber can still have Stackable SOCs.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AddOnSocListType">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="addOnSocInfo" type="tns:AddOnSocInfoType">
        <xs:annotation>
          <xs:documentation>Unique ID created by OVM to identify request received from calling front end application.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AddOnSocInfoType">
    <xs:sequence>
      <xs:element minOccurs="0" name="soc" type="tns:ServiceOrderCodeType">
        <xs:annotation>
          <xs:documentation>Feature code that was passed in the input.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="socDesc" type="tns:ServiceOrderDescriptionType">
        <xs:annotation>
          <xs:documentation>Customer friendly name of the associated feature.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="socLongDesc" type="tns:ServiceOrderLongDescriptionType">
        <xs:annotation>
          <xs:documentation>Customer friendly description of the associated feature.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="optionLongDesc" type="tns:OptionDescriptionDetailType">
        <xs:annotation>
          <xs:documentation>Customer friendly description of the associated feature.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="banLevelCodeInd" type="tns:BillingAccountLevelSocRequiredFlagType">
        <xs:annotation>
          <xs:documentation>True indicates that associated feature is an account level feature.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="cellularSupportInd" type="tns:FeatureSupportedFlagType">
        <xs:annotation>
          <xs:documentation>Indicates that associated feature requires a phone number resource</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="dispatchSupportInd" type="tns:FeatureSupportedFlagType">
        <xs:annotation>
          <xs:documentation>Indicates that the associated feature requires a push-to-talk resource</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="packetDataSupportInd" type="tns:PacketDataSupportTypeCodeType">
        <xs:annotation>
          <xs:documentation>Indicates that the associated feature requires a data access resource.

Valid values:
Y – Yes
N – No
B – (iDEN private IP)
P – (iDEN public IP)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="cdmaPacketDataSupportInd" type="tns:FeatureSupportedFlagType">
        <xs:annotation>
          <xs:documentation>Indicates that the associated feature requires a CDMA IP resource.  </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="recurringCharge" type="tns:MonthlyRecurringChargeType">
        <xs:annotation>
          <xs:documentation>Choice between lts-monthly-price, the monthly recurring charge of the associated feature.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="ltsRecurringChargeList" type="tns:LtsRecurringChargeListType">
        <xs:annotation>
          <xs:documentation>Choice between option-price, list of monthly recurring charges for each subscriber rank of the associated sharing feature.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="oneTimeCharge" type="tns:ChargeAmountType">
        <xs:annotation>
          <xs:documentation>Indicates the amount of an applicable one time charge in order to select the associated feature</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="sharingGroupId" type="tns:ServiceOrderCodeType">
        <xs:annotation>
          <xs:documentation>Sharing Group Identifier</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="optionPriority" type="tns:PriorityLevelType">
        <xs:annotation>
          <xs:documentation>Can be optionally used to determine sorting order of the options returned</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="mutuallyExclusiveInd" type="tns:FeatureSupportedFlagType">
        <xs:annotation>
          <xs:documentation>?</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="contractMonths" type="tns:NumberOfContractMonthsType">
        <xs:annotation>
          <xs:documentation>Indicates the minimally required service agreement for new subscriber activations for the associated feature.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="allowedNumberRanges" type="tns:NumberRangeQuantityType">
        <xs:annotation>
          <xs:documentation>Indicates maximum number of phone ranges if feature is a mobile to office type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="numberRangeSize" type="tns:NumberRangeSizeType">
        <xs:annotation>
          <xs:documentation>Indicates maximum number of phone ranges if feature is a mobile to office type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="productTypeList" type="tns:ProductTypeListType">
        <xs:annotation>
          <xs:documentation>Structure provides product type information.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="corpRequiredInd" type="tns:BillingAccountLevelSocRequiredFlagType">
        <xs:annotation>
          <xs:documentation>Indicates if the associated feature is required to be chosen because of the corp gov id provided in the Options Request.  </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="socEquivalentInd" type="tns:PlanEquivalentFlagType">
        <xs:annotation>
          <xs:documentation>Indicates if associated feature is the technology equivalent (based on brand-type) of the subscriber’s existing feature.  </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="sourceSoc" type="tns:ServiceOrderCodeType">
        <xs:annotation>
          <xs:documentation>Subscriber’s feature that is the technology equivalent of the associated feature (option ID)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="affinityInfo" type="tns:AffinityInfoType">
        <xs:annotation>
          <xs:documentation>Structure provides detail affinity information.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="requiredPrimaryBundleSoc" type="tns:ServiceOrderCodeType">
        <xs:annotation>
          <xs:documentation>Identifies the associated primary SOC code for productId(services) defined as SBD</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="maxSecondaryBundleSocs" type="tns:BundleServiceOrderCodeCountType">
        <xs:annotation>
          <xs:documentation>Identifies the maximum number of secondary bundle SOCs can be added for productId(services) defined as SBD</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="socCategory" type="tns:ServiceCategoryNameType">
        <xs:annotation>
          <xs:documentation>Filterable category of the associated feature.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="featureList" type="tns:FeatureLIstType">
        <xs:annotation>
          <xs:documentation>Structure provides feature information</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="ivrSocDesc" type="tns:ServiceOrderLongDescriptionType">
        <xs:annotation>
          <xs:documentation>This is IVR specific price plan description which is a match to an existing recording in the IVR.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="socCategorySequenceNumber" type="tns:DisplayOrderSequenceNumberType">
        <xs:annotation>
          <xs:documentation>Identifies the display order of each distinct option-category</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="categoryRequired" type="tns:BinaryExpressionType">
        <xs:annotation>
          <xs:documentation>Indicates whether the option-id(s) within the associated are required for at least one selection (Initially this will represent the Daily Offer category).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="serviceManagementInd" type="tns:BinaryExpressionType">
        <xs:annotation>
          <xs:documentation>This indicates whether the option is a service manament add on.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="prepaidSocAttributeInfo" type="tns:PrepaidSocAttributeInfoType">
        <xs:annotation>
          <xs:documentation>Structure provides attributes for VMU add-ons.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="socType" type="tns:ServiceOrderCodeTypeCodeType">
        <xs:annotation>
          <xs:documentation>SOC type or the service type of the SOC.
Examples: 
B - BUY-ONE-GET-ONE-SOC
O- OPTIONAL-SOC
P - PRICE-PLAN
M - REDUCED-PRICE-PLAN
S - REDUCED-SOC
R - REGULAR-SOC

For prepaid subscribers, soc type ‘R’ implies an add-on and O implies a bolt-on.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AffinityAttributeInfoType">
    <xs:sequence>
      <xs:element name="name" type="tns:AffinityAttributeNameType">
        <xs:annotation>
          <xs:documentation>Affinity Attribute name</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="code" type="tns:AttributeKeyType">
        <xs:annotation>
          <xs:documentation>Affinity Attribute code</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="type" type="tns:AffinityAttributeTypeCodeType">
        <xs:annotation>
          <xs:documentation>C = Character, one char only.
N = Integer.
T = Date, with format MM/dd/yyyy
D = Date and Time with format MM/dd/yyyy HH:mm:ss
M = Decimal, meaning has exactly 2 digits after the decimal digit (3.25, 1.99, 19999.33 etc.)
S = String.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="length" type="tns:AffinityAttributeSizeQuantityType">
        <xs:annotation>
          <xs:documentation>Length of affinity attribute</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="requiredInd" type="tns:AffinityAttributeRequiredFlagType">
        <xs:annotation>
          <xs:documentation>True means required.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="mask" type="tns:AffinityAttributeMaskType">
        <xs:annotation>
          <xs:documentation>The mask of the affinity attribute</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="pattern" type="tns:AffinityAttributePatternTextType">
        <xs:annotation>
          <xs:documentation>Valid values are:  Number = #### Decimal(2) = ##,###.##
String = !!!
String = ^^^^ 
String = ###
String = aaaa
String = XXXXXX
Char = All String formats
Date = mm/dd/yyyy
Datetime = mm/dd/yyyy HH:mm:ss </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="value" type="tns:AffinityAttributeValueTextType">
        <xs:annotation>
          <xs:documentation>Contains the default value for the attribute that may or may not be editable based on the editable tag.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="editableInd" type="tns:AffinityAttributeEditableFlagType">
        <xs:annotation>
          <xs:documentation>True means editable.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AffinityAttributeListType">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="affinityAttributeInfo" type="tns:AffinityAttributeInfoType">
        <xs:annotation>
          <xs:documentation>Affinity Attribute Info Structure</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="AffinityInfoType">
    <xs:sequence>
      <xs:element minOccurs="0" name="affinityRegistrationSoc">
        <xs:annotation>
          <xs:documentation>Name of the affinity registration SOC.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:AffinityRegistrationServiceOrderCodeType">
            <xs:maxLength value="9" />
          </xs:restriction>
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="affinityName" type="tns:AffinityNameType">
        <xs:annotation>
          <xs:documentation>Name of the affinity </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="affinityAttributeList" type="tns:AffinityAttributeListType">
        <xs:annotation>
          <xs:documentation>Affinity Attribute List Structure</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="BanInfoType">
    <xs:sequence>
      <xs:element minOccurs="0" name="ban">
        <xs:annotation>
          <xs:documentation>Ensemble Billing Account Number.

Required for existing customers and if the MDN is not provided.</xs:documentation>
        </xs:annotation>
        <xs:simpleType>
          <xs:restriction base="tns:BillingAccountNumberType" />
        </xs:simpleType>
      </xs:element>
      <xs:element minOccurs="0" name="mdn" type="tns:MobileDirectoryNumberStringType">
        <xs:annotation>
          <xs:documentation>MDN of the customer if they havn an active account.  
Required for existing customers and if the BAN is not provided.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:choice>
        <xs:sequence>
          <xs:element name="accountType" type="tns:BillingAccountTypeCodeType">
            <xs:annotation>
              <xs:documentation>The type of account to be used to determine which plans to return.
Valid values:
I, B, C, G
Both accountType and accountSubType need to be passed if customerTypeDesc is not provided in input.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="accountSubType" type="tns:BillingAccountSubTypeCodeType">
            <xs:annotation>
              <xs:documentation>Subscription type to be used to determine which plans to return.

Valid values:
I, B, H, N, G, 4

Both accountType and accountSubType need to be passed if customerTypeDesc is not provided in input.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
        <xs:element name="customerTypeDesc">
          <xs:annotation>
            <xs:documentation>Specifies customer type if account type and sub type are not provided.

Valid values:
INDIVIDUAL
BUSINESS
CORPORATE_INDIVIDUAL
CORPORATE_BUSINESS
GOVERNMENT_INDIVIDUAL
GOVERNMENT_BUSINESS</xs:documentation>
          </xs:annotation>
          <xs:simpleType>
            <xs:restriction base="tns:CustomerTypeCodeType">
              <xs:minLength value="1" />
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:choice>
      <xs:element minOccurs="0" name="creditClass" type="tns:CreditClassCodeType">
        <xs:annotation>
          <xs:documentation>Credit class of the subscriber</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="corpId" type="tns:CorporationIdentifierType">
        <xs:annotation>
          <xs:documentation>The corporate id and government id of the associated subscriber.
Should be provided in input if customer type is Corporate or Government.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="CallingApplicationInfoType">
    <xs:sequence>
      <xs:element name="pin" type="tns:PersonalIdentificationNumberType">
        <xs:annotation>
          <xs:documentation>Combination of vendorCode and pin is used to differentiate the calling system (channel).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="vendorCode" type="tns:VendorCodeType">
        <xs:annotation>
          <xs:documentation>Combination of vendorCode and pin is used to differentiate the calling system (channel).</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="orderId" type="tns:ActivationOrderIdentifierType">
        <xs:annotation>
          <xs:documentation>Activation Order Identifier uniquely identifies an Activation Order.
</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="storeId" type="tns:RetailStoreIdentifierType">
        <xs:annotation>
          <xs:documentation>Identifies store for reporting purposes.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="associateId" type="tns:AssociateIdentifierType">
        <xs:annotation>
          <xs:documentation>Identifies associate for reporting purposes.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="brandCode" type="tns:BrandCodeType">
        <xs:annotation>
          <xs:documentation>S - Sprint
N - Nextel
Defaulted to 'S' if not provided</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DeviceInfoType">
    <xs:choice>
      <xs:element name="itemId" type="tns:ModelNumberType">
        <xs:annotation>
          <xs:documentation>Phone model id of the device that is or will be associated with the subscription.
Required if MEID or HSN is not provided.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="meid" type="tns:MobileEquipmentIdentifierType">
        <xs:annotation>
          <xs:documentation>Mobile Equipment Id for CDMA device.
Required if ModelId or HSN is not provided.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="msn" type="tns:DeviceSerialNumberType">
        <xs:annotation>
          <xs:documentation>iDEN equipment id for an iDEN device.
Required if ModelId or MEID is not provided.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="macId" type="tns:MediumAccessControlAddressType">
        <xs:annotation>
          <xs:documentation>Medium Access Control Identifier
Choice element – Required if none of the other elements in deviceInfo are provided.
</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:choice>
  </xs:complexType>
  <xs:complexType name="FeatureInfoType">
    <xs:sequence>
      <xs:element name="featureCode" type="tns:FeatureCodeType">
        <xs:annotation>
          <xs:documentation>Component of the feature (option-id) , For internal use only</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="featureDesc" type="tns:FeatureDescriptionType">
        <xs:annotation>
          <xs:documentation>Description of the feature component, For internal use only</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="allowedNumberRanges" type="tns:NumberRangeQuantityType">
        <xs:annotation>
          <xs:documentation>Indicates maximum number of phone ranges if associated feature is a mobile to office type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="numberRangeSize" type="tns:NumberRangeSizeType">
        <xs:annotation>
          <xs:documentation>Indicates maximum size of the phone range if associated feature is a mobile to office type</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="msisdnInd" type="tns:FeatureSupportedFlagType">
        <xs:annotation>
          <xs:documentation>For internal use only</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="productTypeList" type="tns:ProductTypeListType">
        <xs:annotation>
          <xs:documentation>Structure providing the product info</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="FeatureLIstType">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" name="featureInfo" type="tns:FeatureInfoType" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LtsRecurringChargeInfoType">
    <xs:sequence>
      <xs:element name="rank" type="tns:LinesToShareRankType">
        <xs:annotation>
          <xs:documentation>Indicates the lines to share pricing  rank, </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="recurringCharge" type="tns:MonthlyRecurringChargeType">
        <xs:annotation>
          <xs:documentation>Lines to share recurring charges.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="LtsRecurringChargeListType">
    <xs:sequence>
      <xs:element minOccurs="0" name="maxSubscribersAllowed" type="tns:NumberOfSubscriptionsType">
        <xs:annotation>
          <xs:documentation>Indicates the maximum number of subscribers that can share the associated feature.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element maxOccurs="unbounded" name="ltsRecurringChargeInfo" type="tns:LtsRecurringChargeInfoType">
        <xs:annotation>
          <xs:documentation>Lines to share recurring charges info structure.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OptionDescriptionDetailType">
    <xs:sequence>
      <xs:element minOccurs="0" name="optionDetail" type="tns:ServiceOrderLongDescriptionType">
        <xs:annotation>
          <xs:documentation>The business provided detailed information about the plan (This is for the Daily SOC as an exception.)</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="optionLearnMore" type="tns:ServiceOrderLongDescriptionType">
        <xs:annotation>
          <xs:documentation>This includes a configurable link to ‘Learn More’ for the plan (This is for the Daily SOC as an exception.) </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="productDisclosure" type="tns:ProductDisclosureTextType">
        <xs:annotation>
          <xs:documentation>This is the product disclosure information
This is the business provided information in detail about the Plan</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="OrderInfoType">
    <xs:sequence>
      <xs:element name="orderType" type="tns:OrderValidationModuleOrderTypeCodeType">
        <xs:annotation>
          <xs:documentation>The type of order to be placed.

Valid values:
NEW - when requesting plans for activation of a new customer account with a new subscriber.
ADD_ON - when requesting plans for adding a subscription to an existing account.
SERVICE_ONLY - when requesting plans for a Plan swap without ESN swap
UPGRADE - when requesting plans for an ESN swap with a plan swap.
REPLACE - when requesting plans for ‘upgrade now’ orders.  For example, incase of the hot device swap transaction. </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="salesChannel" type="tns:SalesChannelIdentifierType">
        <xs:annotation>
          <xs:documentation>Sales channel requesting the plans for the associated subscription.

Valid values:
2 – Online authorized retailer
BSP – Business solution partner</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="numberSubscribers" type="tns:NumberOfSubscriptionsType">
        <xs:annotation>
