					<xs:documentation>Equiment sub group quantity </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="buyPrice" type="ItemRetailPriceType">
				<xs:annotation>
					<xs:documentation>price+additionalDiscount</xs:documentation>
				</xs:annotation>
			</xs:element>
