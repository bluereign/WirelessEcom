<?xml version="1.0" encoding="UTF-8"?><!-- edited with XMLSpy v2012 rel. 2 sp1 (x64) (http://www.altova.com) by Sprint Employee (SPRINT) --><!--Main document view.--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://nextel.com/ovm" targetNamespace="http://nextel.com/ovm" elementFormDefault="qualified" attributeFormDefault="unqualified">	<xs:include schemaLocation="ovm-common.xsd"/>	<xs:complexType name="AccountValidationRequest">		<xs:sequence>			<xs:element name="last-name" minOccurs="0">				<xs:simpleType>					<xs:restriction base="xs:string">						<xs:minLength value="2"/>						<xs:maxLength value="50"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:choice>				<xs:element name="account-number" type="AccountNumber"/>				<xs:element name="reference-ptn" type="PhoneNumber"/>				<xs:element name="sim" type="RequiredStringMax20"/>				<xs:element name="meid" type="MEID"/>				<xs:element name="mac-id" type="MACID"/>				<!--GBL12.2 PJ006784 changes-->				<xs:element name="icc-id" type="IntegratedCircuitCardIdentifierType">					<xs:annotation>						<xs:documentation>Integrated Circuit Card Identifier is a uniqe identifier of the smart card used in mobile terminals in CDMA networks.</xs:documentation>					</xs:annotation>				</xs:element>				<!--end of GBL 12.2 changes-->			</xs:choice>			<xs:element name="requested-devices" type="RequestedDevices" minOccurs="0">				<!--For customers requesting the available pricing for select phone models -->			</xs:element>			<xs:element name="single-subscriber" type="xs:boolean" minOccurs="0">				<!--Indicates if rebates are to be sent only for the subscriber of the PTN sent in request or not.-->			</xs:element>			<xs:element name="sec-pin" type="RequiredStringSecPin" minOccurs="0"/>			<xs:element name="sec-answer" type="AnswerType" minOccurs="0"/>			<xs:choice minOccurs="0">				<xs:element name="ssn">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:whiteSpace value="collapse"/>							<xs:pattern value="\d{4}"/>						</xs:restriction>					</xs:simpleType>				</xs:element>				<xs:element name="federal-tax-id">					<xs:simpleType>						<xs:restriction base="xs:string">							<xs:whiteSpace value="collapse"/>							<xs:pattern value="\d{4}"/>						</xs:restriction>					</xs:simpleType>				</xs:element>			</xs:choice>			<xs:element name="billing-zip" type="ZipCode" minOccurs="0"/>			<xs:element name="returnBogxInd" type="xs:boolean" minOccurs="0"/>			<xs:element name="upgrade-device" minOccurs="0">				<xs:complexType>					<xs:choice>						<xs:element name="hsn" type="HSN">							<!--Handset serial number.-->						</xs:element>						<xs:element name="meid" type="MEID"/>						<xs:element name="mac-id" type="MACID"/>						<xs:element name="model-id" type="RequiredStringMax30">							<!--The phone's model ID.  (e.g. i90c)-->						</xs:element>					</xs:choice>				</xs:complexType>			</xs:element>		</xs:sequence>		<!--Account validation request message.-->	</xs:complexType>	<xs:element name="doAccountValidation" type="AccountValidationRequest"/>	<xs:complexType name="AccountValidationResponse">		<xs:sequence>			<xs:element name="account-status" type="AccountStatus">				<!--Account status.  Values can be: ACTIVE, TENTATIVE, CLOSED, CANCELLED, and SUSPENDED.-->			</xs:element>			<xs:element name="account-status-date" type="xs:dateTime" minOccurs="0">				<!--When account status was last updated.-->			</xs:element>			<xs:element name="account-number" type="AccountNumber" minOccurs="0">				<!--Account number-->			</xs:element>			<xs:element name="number-handsets-approved" type="xs:unsignedInt" minOccurs="0">				<!--number of handsets approved on account-->			</xs:element>			<xs:element name="account-info" type="AccountInfo" minOccurs="0">				<!--Account Info-->			</xs:element>			<xs:element name="rebateBogxInfo" type="RebateBogxInfoType" minOccurs="0"/>			<xs:element name="billing-name" type="BillingName" minOccurs="0">				<!--Account holder's first and last name or company name-->			</xs:element>			<xs:element name="billing-address" type="BillingAddress" minOccurs="0"/>			<xs:element name="account-balance" minOccurs="0">				<xs:simpleType>					<xs:restriction base="xs:decimal">						<xs:fractionDigits value="2"/>					</xs:restriction>				</xs:simpleType>			</xs:element>			<xs:element name="payment-due-date" type="xs:date" minOccurs="0"/>			<xs:element name="balance-expiration-date" type="xs:date" minOccurs="0"/>		</xs:sequence>		<!--Account validation response message.-->	</xs:complexType>	<xs:element name="doAccountValidationResponse" type="AccountValidationResponse"/></xs:schema>