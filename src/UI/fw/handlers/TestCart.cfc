<cfcomponent output="false" extends="BaseHandler">		<cfproperty name="DBuilderCart" inject="id:DBuilderCart" />	<cfproperty name="DBuilderCartHelper" inject="id:DBuilderCartHelper" />	<cfproperty name="DBuilderCartItem" inject="id:DBuilderCartItem" />	<cfproperty name="DBuilderCartPriceBlock" inject="id:DBuilderCartPriceBlock" />		<cffunction name="index" returntype="void" output="false">		<cfargument name="event">		<cfargument name="rc">		<cfargument name="prc">				<cfset var theCart = getModel('dBuilderCart').init() />		<cfset session.dBuilderCart = thecart />		<cfset event.setView('TestCart/index') />	</cffunction>			<cffunction name="cart_input" returntype="void" output="false">		<cfargument name="event">		<cfargument name="rc">		<cfargument name="prc">				<cfset event.setView('TestCart/cart_input') />	</cffunction>		<cffunction name="applyZip" returntype="void" output="false">		<cfargument name="event">		<cfargument name="rc">		<cfargument name="prc">		<cfset var local = {} />				<cfparam name="rc.zipcode" default="00000" />				<cfset session.dbuilderCart.setZipCode(rc.zipcode) />						<cfset event.setView('testcart/index') />	</cffunction><!------------------------------------------- PRIVATE EVENTS ------------------------------------------></cfcomponent>