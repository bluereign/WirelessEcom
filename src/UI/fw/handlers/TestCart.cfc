<cfcomponent output="false" extends="BaseHandler">		<cfproperty name="DBuilderCart" inject="id:DBuilderCart" />	<cfproperty name="DBuilderCartHelper" inject="id:DBuilderCartHelper" />	<cfproperty name="DBuilderCartItem" inject="id:DBuilderCartItem" />	<cfproperty name="DBuilderCartPriceBlock" inject="id:DBuilderCartPriceBlock" />		<cffunction name="index" returntype="void" output="false">		<cfargument name="event">		<cfargument name="rc">		<cfargument name="prc">				<!---<cfset session.cart = getModel('dBuilderCart').init() />		<cfset session.cartHelper = getModel('dBuilderCartHelper').init() />--->				<cfset session.cart = createObject('component','cfc.model.cart').init() />		<cfset session.cartHelper = createObject('component','cfc.model.carthelper').init() />		<cfset event.setView('TestCart/index') />	</cffunction>			<cffunction name="cart_input" returntype="void" output="false">		<cfargument name="event">		<cfargument name="rc">		<cfargument name="prc">				<cfset event.setView('TestCart/cart_input') />	</cffunction>		<cffunction name="applyZip" returntype="void" output="false">		<cfargument name="event">		<cfargument name="rc">		<cfargument name="prc">		<cfset var local = {} />				<cfparam name="rc.zipcode" default="00000" />				<cfset session.cart.setZipCode(rc.zipcode) />						<cfset event.setView('testcart/index') />	</cffunction>	<cffunction name="CartHelperTest" returntype="void" output="false">		<cfargument name="event">		<cfargument name="rc">		<cfargument name="prc">		<cfset var cart = createObject("Component","cfc.model.cart").init() />		<cfset var carthelper = createObject("Component","cfc.model.carthelper").init() />		<cfset session.cart = cart />		<cfset session.carthelper = carthelper />				<cfset session.carthelper.clearCart() />		<cfset session.cart.setZipCode("98006") />		<cfset session.carthelper.AddLineToCart() />				<cfset event.setView('TestCart/index') />	</cffunction>		<!------------------------------------------- PRIVATE EVENTS ------------------------------------------></cfcomponent>