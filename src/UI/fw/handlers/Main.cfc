<cfcomponent output="false" extends="BaseHandler">		<!--- Use CFProperty to declare beans for injection.  By default, they will be placed in the variables scope --->	<cfproperty name="assetPaths" inject="id:assetPaths" scope="variables" />		<!--- Default Action --->	<cffunction name="index" returntype="void" output="false" hint="Sample Event">		<cfargument name="event">		<cfargument name="rc">		<cfargument name="prc">				<cfset event.setView("Main/sample")>	</cffunction>		<cffunction name="test" returntype="void" output="false">		<cfargument name="event">		<cfargument name="rc">		<cfargument name="prc">				<cfscript>			//Both of these essentially do the same thing			event.setValue("assetPaths",variables.assetPaths);			rc.assetPaths = assetPaths;						event.setView('main/test');		</cfscript>	</cffunction>	<cffunction name="PageNotFound" returntype="void" output="false">		<cfargument name="event">		<cfargument name="rc">		<cfargument name="prc">				<cfscript>			//TODO: log request event						//Relocate to home page for invalid FW events until we build out the page not found view			relocate('/index.cfm');		</cfscript>	</cffunction>	<cffunction name="HandleException" returntype="void" output="false">		<cfargument name="event">		<cfargument name="rc">		<cfargument name="prc">        <cfset event.paramValue( 'bFriendlyErrorPages', true ) />        <cfset rc = event.getCollection() />			<cfscript>			if ( rc.bFriendlyErrorPages ) {				sendErrorEmail( rc.exceptionBean, rc );				relocate('/index.cfm/go/error');			} else {				event.setView('main/errorDump').noLayout();			};		</cfscript>	</cffunction>	<!--- TO DO: Move this into it's own service layer --->	<cffunction name="sendErrorEmail" access="public" output="false" returntype="void">		<cfargument name="exceptionBean" />		<cfargument name="exceptionContext" />        <cfparam name="cgi" type="struct" default="#structNew()#">        <cftry>            <cfmail from="#application.errorFromAddress#" to="#application.errorEmailList#" subject="[MembershipWireless.com] (#cgi.server_name#) Application Error" type="html">            	<cfdump var="#arguments.exceptionBean.getMemento()#" label="EXCEPTION parameters" />				<hr>				<cfdump var="#arguments.exceptionContext#" label="Request (rc) parameters" />				<hr>				<cfdump var="#cgi#" label="CGI parameters" />				<hr>				Checkout Reference Number: <cfif IsDefined('session.checkout.referenceNumber')>#session.checkout.referenceNumber#</cfif><br>				Session User Email: <cfif IsDefined('session.currentUser')>#session.currentUser.getEmail()#</cfif><br>				Session User ID: <cfif IsDefined('session.currentUser')>#session.currentUser.getUserId()#</cfif><br>            </cfmail>            <cfcatch>            	<!--- TODO: log errors to file --->            </cfcatch>        </cftry>    </cffunction>		<!------------------------------------------- PRIVATE EVENTS ------------------------------------------></cfcomponent>