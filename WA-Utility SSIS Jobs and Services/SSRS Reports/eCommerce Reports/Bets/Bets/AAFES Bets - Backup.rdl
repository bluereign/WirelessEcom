      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>DECLARE @ActualOrders int

SELECT @ActualOrders = count(sod.OrderDetailId)
FROM salesorder.[order] so
INNER JOIN salesorder.OrderDetail sod ON sod.OrderId = so.OrderId AND sod.OrderDetailType = 'd'
WHERE so.Status &gt; '0'
AND so.OrderDate BETWEEN '2013-11-26 21:00:00.000' AND '2013-12-02 21:00:00.000'


DECLARE @Bets TABLE (Name varchar(20), Guess int)
INSERT INTO @Bets (Name, Guess)
VALUES ('Candice','90')
,('Randolph','200')
,('Naomi','300')
,('Chris','189')
,('Steve','450')
,('Patrick','50')
,('Jerrod','250')

--DECLARE @LikelyTotal int
--SELECT @LikelyTotal = (AVG(@ActualOrders/(DAY(GETDATE()-7))) * SUM(datediff(day, GETDATE(), dateadd(month, 1, GETDATE())) - DAY(GETDATE()))) + @ActualOrders

--SELECT @ActualOrders AS 'Current Order Total'--, @LikelyTotal AS 'Likely Order Total'

SELECT Name, Guess, @ActualOrders AS 'Actual',
--CASE WHEN SUM(@ActualOrders-Guess) &gt; '0' THEN 'OVER' WHEN SUM(@ActualOrders-Guess) &lt; '0' THEN 'UNDER' ELSE 'JACKPOT!' END AS 'Over/Under',
CASE WHEN SUM(@ActualOrders-Guess) &gt; '0' THEN '+' + CONVERT(varchar(10),SUM(@ActualOrders-Guess))
WHEN SUM(@ActualOrders-Guess) &lt; '0' THEN '-' + SUBSTRING(CONVERT(varchar(10),SUM(@ActualOrders-Guess)),2,11)
ELSE CONVERT(varchar(10),SUM(@ActualOrders-Guess)) END AS 'Over/Under'
--,CASE WHEN SUM(@ActualOrders-Guess) &gt; '0' THEN SUM(@ActualOrders-Guess) WHEN SUM(@ActualOrders-Guess) &lt; '0' THEN SUBSTRING(CONVERT(varchar(10),SUM(@ActualOrders-Guess)),2,11) ELSE CONVERT(varchar(10),SUM(@ActualOrders-Guess)) END AS 'Amount Off'
--,row_number() over(order by SUM(@ActualOrders-Guess) DESC) AS 'Winning!'
 FROM @Bets
GROUP BY Name, Guess
ORDER BY CASE WHEN SUM(@ActualOrders-Guess) &gt; '0' THEN SUM(@ActualOrders-Guess)
WHEN SUM(@ActualOrders-Guess) &lt; '0' THEN SUBSTRING(CONVERT(varchar(10),SUM(@ActualOrders-Guess)),2,11)
ELSE CONVERT(varchar(10),SUM(@ActualOrders-Guess)) END</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
