        <Field Name="Meaning">
          <DataField>Meaning</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT DISTINCT

	Convert(varchar,o.OrderDate,101) as OrderDate
	, CASE WHEN o.PcrDate IS NULL
		THEN
			Convert(varchar,DATEADD(d, 2, o.OrderDate), 101)
		ELSE
			Convert(varchar,DATEADD(d, 2, o.PcrDate), 101)
		END SLADate

	,  CASE WHEN o.PcrDate IS NULL
		THEN (SELECT COUNT(*) FROM admin.WACalendar wac WHERE wac.BType = 1 AND wac.BDate &gt; o.OrderDate AND wac.BDate &lt; GETDATE() AND o.Status IN ('1','2'))	    
	   WHEN Convert(varchar,DATEADD(d, 2, o.PcrDate), 101) &lt; CONVERT(VARCHAR,GETDATE(),101)
	    THEN  (SELECT '-' + COUNT(*) FROM admin.WACalendar wac WHERE wac.BType = 1 AND wac.BDate &gt; o.OrderDate AND wac.BDate &lt; GETDATE() AND o.Status IN ('1','2'))	    
	   ELSE (SELECT COUNT(*) FROM admin.WACalendar wac WHERE wac.BType = 1 AND wac.BDate &gt; o.PcrDate   AND wac.BDate &lt; GETDATE() AND o.Status IN ('1','2'))
	    END DaysFromSLA
,

	 o.OrderId
	, c.CompanyName Carrier
	, o.ActivationType
	--, Convert(varchar, wa.ActivationDate,101)as ActivationDate
	, CASE o.Status
		WHEN 0 THEN 'Pending'
		WHEN 1 THEN 'Submitted'
		WHEN 2 THEN 'Order Placed'
		WHEN 3 THEN
			CASE o.GERSStatus
				WHEN 2 THEN 'Packing'
				WHEN 3 THEN 'Shipped'
			ELSE 'Processing'
			END
		WHEN 4 THEN 'Cancelled'
		WHEN 5 THEN 'Other, could this be pending?'
		ELSE ''
	  END Status
	,o.GersStatus
	,(SELECT OrderStatus fROM salesorder.orderstatus WHERE OrderType = 'GERS' AND OrderStatusId = o.GersStatus) AS 'Meaning'


FROM salesorder.[Order] o
INNER JOIN catalog.Company c ON c.CarrierId = o.CarrierId
INNER JOIN salesorder.WirelessAccount wa ON wa.OrderId = o.OrderId
INNER JOIN salesorder.ShipMethod sm ON o.ShipMethodId = sm.ShipMethodId
WHERE 
	o.Status NOT IN ('0','4')
	AND o.GersStatus &lt; '0'
ORDER BY OrderId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
