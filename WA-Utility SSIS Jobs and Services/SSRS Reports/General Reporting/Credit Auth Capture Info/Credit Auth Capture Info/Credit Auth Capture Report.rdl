  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="timesenttogers">
          <DataField>timesenttogers</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="GersRefNum">
          <DataField>GersRefNum</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CreditCardAuthorizationNumber">
          <DataField>CreditCardAuthorizationNumber</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>Production</DataSourceName>
        <CommandText>select o.timesenttogers, O.GersRefNum, P.CreditCardAuthorizationNumber
from salesorder.Payment P
       inner join salesorder.[Order] O ON P.OrderId = O.OrderId
WHERE GERSRefNum is not null and ISNUMERIC(P.CreditCardAuthorizationNumber) = 1 and LEN(P.CreditCardAuthorizationNumber) = 6
AND o.timesenttogers BETWEEN @StartDate AND @EndDate</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
