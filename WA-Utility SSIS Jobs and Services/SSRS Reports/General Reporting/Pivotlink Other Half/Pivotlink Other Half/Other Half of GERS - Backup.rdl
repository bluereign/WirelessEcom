  <PageHeight>11in</PageHeight>
  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="OrderDate">
          <DataField>OrderDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="ECOM_Number">
          <DataField>ECOM Number</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="GERS_Invoice_Number">
          <DataField>GERS Invoice Number</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Carrier">
          <DataField>Carrier</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ActivationType">
          <DataField>ActivationType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Handset_Qty">
          <DataField>Handset Qty</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Handset_SKU">
          <DataField>Handset SKU</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Handset_SKU_Description">
          <DataField>Handset SKU Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Handset_COGS">
          <DataField>Handset COGS</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Handset_Retail">
          <DataField>Handset Retail</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Handset_Customer_Price">
          <DataField>Handset Customer Price</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Rateplan_SKU">
          <DataField>Rateplan SKU</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Rateplan_SKU_Description">
          <DataField>Rateplan SKU Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Rateplan_Monthly_Fee">
          <DataField>Rateplan Monthly Fee</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Enhanced_Services__SKU_Description_Fee_">
          <DataField>Enhanced Services (SKU+Description+Fee)</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>WITH UGotServed (OrderId, GroupNumber, ServiceTotal,EnhancedServicesDescription) AS (
SELECT DISTINCT
sod.OrderId
, sod.GroupNumber
, COUNT(sod.GroupNumber) AS 'Service Total'
,

STUFF ((
SELECT '|' + (ISNULL(shoop.GersSku,'[No SKU]') + ' - ' + shoop.ProductTitle + ' - ' + CONVERT(varchar(10),ser.MonthlyFee))
FROM salesorder.OrderDetail shoop
INNER JOIN catalog.Product pro ON pro.ProductId = shoop.ProductId
INNER JOIN catalog.Service ser ON ser.ServiceGuid = pro.ProductGuid
WHERE shoop.OrderId = so.Orderid AND shoop.GroupNumber = sod.GroupNumber AND shoop.OrderDetailType = 's'
FOR XML PATH ('')),1,1,'') AS 'Enhanced Services Description'
FROM salesorder.[Order] so
INNER JOIN salesorder.OrderDetail sod ON sod.OrderId = so.OrderId AND sod.OrderDetailType = 's'
INNER JOIN catalog.Product cp ON cp.ProductId = sod.ProductId
GROUP BY sod.OrderId, sod.GroupNumber, so.OrderId
)





SELECT
      so.TimeSentToGERS AS 'OrderDate'
      ,so.OrderId AS 'ECOM Number'
      ,so.GERSRefNum AS 'GERS Invoice Number'
      ,cc.CompanyName AS 'Carrier'
      ,CASE WHEN cc.CompanyName = 'Boost' THEN 'P'
      WHEN sod.GersSku IN (SELECT GersSku FROM catalog.product cp INNER JOIN catalog.producttag cpt ON cpt.productguid = cp.productguid WHERE Tag LIKE '%prepaid%') THEN 'P'
      ELSE so.ActivationType END AS 'ActivationType'
      ,COUNT(sod.OrderDetailId) AS 'Handset Qty'
      ,sod.GersSku AS 'Handset SKU'
      ,sod.ProductTitle AS 'Handset SKU Description'
      ,sod.COGS AS 'Handset COGS'
      ,sod.RetailPrice AS 'Handset Retail'
      ,sod.NetPrice AS 'Handset Customer Price'
      ,CASE WHEN rpi.GersSku IS NULL AND so.ActivationType IN ('U','A') THEN
      rpn.GersSku ELSE ISNULL(rpi.GersSku,'N/A') END AS 'Rateplan SKU'
      ,CASE WHEN rpi.ProductTitle IS NULL AND so.ActivationType IN ('U','A') THEN
      rpn.ProductTitle ELSE ISNULL(rpi.ProductTitle,'Not Applicable') END AS 'Rateplan SKU Description'
      ,ISNULL(CONVERT(VARCHAR(10),cr.MonthlyFee),'N/A') AS 'Rateplan Monthly Fee'
      ,ISNULL(us.EnhancedServicesDescription,'Not Applicable') AS 'Enhanced Services (SKU+Description+Fee)'
FROM salesorder.[Order] so
INNER JOIN catalog.Company cc ON cc.CarrierId = so.CarrierId
INNER JOIN salesorder.orderdetail sod ON sod.OrderId = so.OrderId AND sod.OrderDetailType = 'd'
LEFT JOIN salesorder.orderdetail rpi ON rpi.OrderId = so.OrderId AND rpi.GroupNumber = sod.GroupNumber AND rpi.OrderDetailType = 'r'
LEFT JOIN salesorder.orderdetail rpn ON rpn.OrderId = so.OrderId AND rpn.GroupNumber = sod.GroupNumber AND rpn.OrderDetailType = 'u'
LEFT JOIN catalog.Product cp ON cp.ProductId = rpi.productid
LEFT JOIN catalog.Rateplan cr ON cr.RateplanGuid = cp.ProductGuid
LEFT JOIN UGotServed us ON us.OrderId = sod.OrderId AND us.GroupNumber = sod.GroupNumber
WHERE so.TimeSentToGERS BETWEEN @StartDate AND DATEADD(DAY, 1, @EndDate)
GROUP BY      so.TimeSentToGERS
      ,so.OrderId
      ,so.GERSRefNum
      ,cc.CompanyName
      ,so.ActivationType
      ,so.ActivationType
      ,sod.GersSku
      ,sod.ProductTitle
      ,rpi.GersSku
      ,rpn.GersSku
      ,rpi.ProductTitle
      ,rpn.ProductTitle
      ,sod.COGS
      ,sod.RetailPrice
      ,sod.NetPrice      ,cr.MonthlyFee
      ,us.EnhancedServicesDescriptionORDER BY 	  so.OrderId	  ,so.ActivationType</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <Timeout>0</Timeout>
      </Query>
