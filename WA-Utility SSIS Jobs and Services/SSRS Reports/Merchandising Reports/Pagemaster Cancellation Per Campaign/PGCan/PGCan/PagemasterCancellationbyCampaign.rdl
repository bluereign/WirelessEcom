  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT CONVERT(VARCHAR(10),@StartDate,101) + '-' + CONVERT(VARCHAR(10),@EndDate,101) AS 'Date Range', cc.CompanyName AS 'Campaign',COUNT(DISTINCT sod.OrderDetailId) AS 'Cancelled' FROM salesorder.[order] so
INNER JOIN salesorder.orderdetail sod ON sod.orderid = so.orderid AND sod.OrderDetailType = 'd'
INNER JOIN campaign.campaign cc ON cc.campaignid = so.campaignid
WHERE so.Status = '4'
AND CONVERT(VARCHAR(10),OrderDate,101) BETWEEN @StartDate AND @EndDate
GROUP BY cc.CompanyName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Date_Range">
          <DataField>Date Range</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Campaign">
          <DataField>Campaign</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Cancelled">
          <DataField>Cancelled</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
