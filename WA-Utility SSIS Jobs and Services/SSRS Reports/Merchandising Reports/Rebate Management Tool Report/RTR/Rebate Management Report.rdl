      <Fields>
        <Field Name="Carrier">
          <DataField>Carrier</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Active">
          <DataField>Active</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Rebate_Name">
          <DataField>Rebate Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Callout">
          <DataField>Callout</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Start_Date">
          <DataField>Start Date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="End_Date">
          <DataField>End Date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SKUs_Tagged">
          <DataField>SKUs Tagged</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT DISTINCT
	cmsc.Name AS 'Carrier'
	,CASE Active	WHEN '0' THEN 'NO' ELSE 'YES' END AS 'Active'
	,cmsr.Name AS 'Rebate Name'
	,util.udf_StripHTML(LinkDescription) AS 'Callout'
	,CONVERT(varchar(10),cmsr.StartDateTime,101) AS 'Start Date'
	,CONVERT(varchar(10),cmsr.EndDateTime,101) AS 'End Date'
	,STUFF(
             (SELECT ', ' + cmsrs.sku
             FROM cms.RebateSKUs cmsrs
             WHERE cmsrs.RebateGUID = cmsr.RebateGUID
              FOR XML PATH (''))
             , 1, 1, '') AS 'SKUs Tagged'
FROM cms.Rebates cmsr
LEFT OUTER JOIN cms.RebateSKUs cmsrs ON cmsrs.RebateGUID = cmsr.RebateGUID
INNER JOIN cms.Carriers cmsc ON cmsc.CarrierID = cmsr.CarrierID
WHERE cmsr.EndDateTime &gt; GETDATE() AND Active = '1'
ORDER BY [End Date] DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
