    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="DateRange">
          <DataField>DateRange</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="No_Orders">
          <DataField>No Orders</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Phones">
          <DataField>Phones</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TotalPhones">
          <DataField>TotalPhones</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>EcomLive</DataSourceName>
        <CommandText>DECLARE @Orders INT;
DECLARE @Phones INT;
DECLARE @CancelledOrders INT;
DECLARE @CancelledPhones INT;

SELECT  @Orders = COUNT(o.OrderId)
FROM  salesorder.[Order] o WITH (NOLOCK) 
WHERE CAST(o.OrderDate AS DATE) &gt;= @StartDate AND CAST(o.OrderDate AS DATE) &lt;= @EndDate
      AND   o.Status IN (1,2,3);
      
SELECT  @CancelledOrders = COUNT(o.OrderId)
FROM  salesorder.[Order] o WITH (NOLOCK) 
WHERE CAST(o.OrderDate AS DATE) &gt;= @StartDate AND CAST(o.OrderDate AS DATE) &lt;= @EndDate
      AND   o.Status IN (4);  
      
SELECT            @Phones = COUNT(od.OrderDetailId)
FROM        salesorder.[Order] o WITH (NOLOCK)
INNER JOIN  salesorder.OrderDetail od WITH (NOLOCK) ON o.OrderId = od.OrderId AND od.OrderDetailType = 'd'
WHERE       CAST(o.OrderDate AS DATE) &gt;= @StartDate AND CAST(o.OrderDate AS DATE) &lt;= @EndDate
            AND   o.Status IN (1,2,3);
            
SELECT            @CancelledPhones = COUNT(od.OrderDetailId)
FROM        salesorder.[Order] o WITH (NOLOCK)
INNER JOIN  salesorder.OrderDetail od WITH (NOLOCK) ON o.OrderId = od.OrderId AND od.OrderDetailType = 'd'
WHERE       CAST(o.OrderDate AS DATE) &gt;= @StartDate AND CAST(o.OrderDate AS DATE) &lt;= @EndDate
            AND   o.Status IN (4);        
            
SET NOCOUNT ON

SELECT      CONVERT(VARCHAR(10), @StartDate, 101) + ' to ' + CONVERT(VARCHAR(10), @EndDate, 101) DateRange,
            'TOTAL ORDERS: ' + CAST(@Orders AS NVARCHAR(50)) + ' - CANCELLED ORDERS: ' + CAST(@CancelledOrders AS NVARCHAR(50)) + ' - CANCELLED PHONES: ' + CAST(@CancelledPhones AS NVARCHAR(50)) [No Orders],
            'GRAND TOTAL OF PHONES:' Phones, @Phones TotalPhones

UNION

SELECT            '', '', od.ProductTitle, COUNT(od.GersSku) PhonesOrdered
FROM        salesorder.[Order] o WITH (NOLOCK) 
INNER JOIN  salesorder.OrderDetail od WITH (NOLOCK) ON o.OrderId = od.OrderId AND od.OrderDetailType = 'd'
WHERE       CAST(o.OrderDate AS DATE) &gt;= @StartDate AND CAST(o.OrderDate AS DATE) &lt;= @EndDate
            AND   o.Status IN (1,2,3)
GROUP BY    od.ProductTitle
</CommandText>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
