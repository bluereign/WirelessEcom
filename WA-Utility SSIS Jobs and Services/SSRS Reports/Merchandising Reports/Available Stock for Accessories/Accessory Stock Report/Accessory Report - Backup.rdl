      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT SV.GERSSKU
,(SELECT Value FROM catalog.property cpy INNER JOIN catalog.product cp ON cp.GersSku = sv.GersSku WHERE Name = 'title' and cpy.ProductGuid = cp.ProductGuid) 'NAME'
,sv.[on hand qty] [On Hand Qty]
,SV.[AVAIL QTY] [UNITS AVAILABLE FOR SALE]
,ISNULL(V1.UNITS_1,0) [SALES PAST DAY]
,ISNULL(V7.UNITS_7,0) [SALES PAST 7 DAYS]
,ISNULL(V14.UNITS_14,0) [SALES PAST 14 DAYS]
, case when convert(int, (ISNULL(V7.UNITS_7,0) * 3.5) -  SV.[AVAIL QTY]) &gt;0
  then convert(int, (ISNULL(V7.UNITS_7,0) * 3.5) -  SV.[AVAIL QTY])   else '0'
  end 'Recommend Order'    
,isnull(SV.NEWPRICE,0) [NEW PRICE]
,isnull(SV.UPGRADEPRICE,0) [UPGRADE PRICE]	
,isnull(SV.ADDALINEPRICE,0) [ADD A LINE PRICE]
,isnull(SV.COGSPRICE,0) [COGS PRICE] 
,isnull(SV.HOLDBACK,0) [HOLD BACK]
,SV.PRODID [PRODUCT ID]
FROM SALESAccessory_VIEW SV  left  JOIN FourteendaysSalesAccessory_View  V14 ON 
SV.GERSSKU=V14.GERSSKU LEFT JOIN  SevendaysSalesAccessory_View  V7 
ON  SV.GERSSKU=V7.GERSSKU  LEFT JOIN OnedaySalesAccessory_View   V1
ON SV.GERSSKU=V1.GERSSKU
where SV.GERSSKU  not like ('%Kt%') 
AND SV.GersSku NOT IN (SELECT GersSku FROM catalog.Product cp INNER JOIN catalog.ProductTag cpt ON cpt.ProductGuid = cp.ProductGuid WHERE cpt.Tag = 'FREEACCESSORY')
and sv.GersSku IN (SELECT GersSku FROM catalog.Product WHERE Active = '1')
AND RIGHT(sv.GersSku,1) &lt;&gt; 'K' AND RIGHT (SV.GERSSKU,1) &lt;&gt; 'S'
ORDER BY 1 asc</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
