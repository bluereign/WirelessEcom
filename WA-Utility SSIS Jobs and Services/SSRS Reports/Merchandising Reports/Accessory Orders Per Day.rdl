  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="Accessories">
          <DataField>Accessories</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Totals">
          <DataField>Totals</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="GersSku">
          <DataField>GersSku</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>EcomLive</DataSourceName>
        <CommandText>SELECT	
		od.ProductTitle Accessories, 
		od.GersSku
		,count(od.GersSku) Totals
		
FROM	salesorder.[Order] o 
INNER JOIN salesorder.OrderDetail od ON o.OrderId=od.OrderId and od.OrderDetailType='a'
WHERE	 convert(varchar(10), OrderDate,101) between convert(varchar(10),GETDATE()-1,101) and convert(varchar(10),GETDATE()-1,101)
		AND o.Status IN (1,2,3) and od.RetailPrice &lt;&gt;0 and od.ProductTitle is not null 
		and o.ActivationType&lt;&gt;'e'
GROUP BY od.ProductTitle,od.GersSku
order by 1</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
  </DataSets>
