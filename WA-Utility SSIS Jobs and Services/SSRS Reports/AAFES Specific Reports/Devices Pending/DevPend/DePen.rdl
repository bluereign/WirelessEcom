  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SET FMTONLY OFF;

WITH durp (PendingDate,OrderDate,OrderId,ActivationType,CarrierId) AS (
SELECT DISTINCT CONVERT(VARCHAR(10),ChangeDate,101) AS 'PendingDate', OrderDate,OrderId, ActivationType, CarrierId FROM logging.orders 
WHERE Status IN ('1','2') AND GERSStatus = '0' AND ActivationType NOT IN ('E')
AND OrderId NOT IN (SELECT DISTINCT OrderId FROM logging.orders WHERE Status IN ('3','4'))
)


SELECT DISTINCT
	 CONVERT(varchar(10),so.OrderDate,101) AS 'OrderDate'
	 ,DATEDIFF(DAY, CONVERT(varchar(10),so.OrderDate,101), @EndDate) AS 'DaysPending'
      ,sa.MilitaryBase AS 'MilitaryBase'
      ,CASE cc.CompanyName
      WHEN 'AT&amp;T' THEN 'ATT'
      WHEN 'Verizon Wireless' THEN 'Verizon'
      ELSE cc.CompanyName
      END AS 'Carrier'
      ,CASE WHEN cc.CompanyName = 'Boost' THEN 'Prepaid'
      WHEN sod.GersSku IN (SELECT GersSku FROM catalog.product cp INNER JOIN catalog.producttag cpt ON cpt.productguid = cp.productguid WHERE Tag = 'prepaid') THEN 'Prepaid'
      WHEN so.ActivationType IN ('N','A') THEN 'Postpaid'
      WHEN so.ActivationType IN ('R') THEN 'Equipment only'
      WHEN so.ActivationType = 'U' THEN 'Upgrade'
      WHEN so.ActivationType = 'E' THEN 'Exchange'
      ELSE so.ActivationType END AS 'Type'
      ,so.OrderId
      ,COUNT(sod.OrderDetailId) AS 'Qty'
      ,sod.GersSku
      ,sod.ProductTitle AS 'SKU Description'
FROM durp so
INNER JOIN catalog.Company cc ON cc.CarrierId = so.CarrierId
INNER JOIN salesorder.orderdetail sod ON sod.OrderId = so.OrderId AND sod.OrderDetailType = 'd'
INNER JOIN salesorder.[Order] sso ON sso.OrderId = so.OrderId
INNER JOIN salesorder.address sa ON sa.addressguid = sso.shipaddressguid
WHERE CONVERT(date,so.PendingDate,101) BETWEEN DATEADD(DAY,-1,@StartDate) AND DATEADD(DAY,-1,@EndDate)
GROUP BY sa.MilitaryBase,sod.GersSku, sod.ProductTitle, cc.CompanyName, CONVERT(varchar(10),so.PendingDate,101), so.ActivationType,so.OrderId,CONVERT(varchar(10),so.OrderDate,101)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
