        <Field Name="SKU_Description">
          <DataField>SKU Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT
      CONVERT(varchar(10),so.OrderDate,101) AS 'Date'
      ,sa.MilitaryBase
      ,cc.CompanyName AS 'Carrier'
      ,so.ActivationType AS 'Type'
      ,COUNT(sod.OrderDetailId) AS 'Qty'
      ,sod.GersSku
      ,sod.ProductTitle AS 'SKU Description'
FROM salesorder.[Order] so
INNER JOIN catalog.Company cc ON cc.CarrierId = so.CarrierId
INNER JOIN salesorder.orderdetail sod ON sod.OrderId = so.OrderId AND sod.OrderDetailType = 'd'
INNER JOIN salesorder.address sa ON sa.addressguid = so.shipaddressguid
GROUP BY sa.MilitaryBase,sod.GersSku, sod.ProductTitle, cc.CompanyName, CONVERT(varchar(10),so.OrderDate,101), so.ActivationType</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
