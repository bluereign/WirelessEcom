  <DataSets>
    <DataSet Name="ShipBillAddresses">
      <Query>
        <DataSourceName>EcomLive</DataSourceName>
        <CommandText>DECLARE @AddressKeyword nvarchar(250)
DECLARE @FirstName nvarchar(100)
DECLARE @LastName nvarchar(100)
DECLARE @City nvarchar(100)
DECLARE @State nvarchar(2)
DECLARE @Zip nvarchar(10);


with 
bannedaddresses as
(
select distinct Address1 from websecurity.BannedUsers
)
,alladdress as (
select	top 100 percent o.OrderId,
	   (CONVERT(varchar(10),o.OrderDate,101)) OrderDate,
		os.OrderStatus,
		case when bus.Address1 is not null then 'Yes' else 'No' end ShipBanned,
		sa.FirstName ShipFirstName,
		sa.LastName ShipLastName,
		COALESCE (sa.Address1+' '+sa.address2+' '+sa.Address3,sa.Address1+' '+sa.address2, sa.Address1) ShipToAddress,
		sa.City ShipCity,
		sa.State ShipState,
		sa.Zip ShipZip,
		case when bub.Address1 is not null then 'Yes' else 'No' end BillBanned,
		ba.FirstName BillFirstName,
		ba.LastName BillLastName,
		COALESCE (ba.Address1+' '+ba.address2+' '+ba.Address3,ba.Address1+' '+ba.address2, ba.Address1) BillToAddress,
		ba.City BillCity,
		ba.State BillState,
		ba.Zip BillZip
from	salesorder.[Order] o
		inner join salesorder.Address sa
on		o.ShipAddressGuid=sa.AddressGuid
		inner join salesorder.Address ba
on		o.BillAddressGuid=ba.AddressGuid
		inner join dbo.Users u
on		o.UserId=u.[User_ID]
		left outer join bannedaddresses bus
on		sa.Address1=bus.Address1
		left outer join bannedaddresses bub
on		ba.Address1=bub.Address1
		left outer join  salesorder.OrderStatus os
on		o.[Status]=os.OrderStatusId and os.OrderType='WA'
--order by sa.State, sa.City,sa.Address1
)

SELECT * FROM ALLADDRESS WHERE
	(ShipToAddress LIKE '%' + @AddressKeyWord + '%'
	OR BillToAddress LIKE '%' + @AddressKeyWord + '%')
	AND (ShipCity LIKE '%' + @City + '%'
		OR BillCity LIKE '%' + @City + '%')
	AND (ShipState LIKE '%' + @State + '%'
		OR BillState LIKE '%' + @State + '%')</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
