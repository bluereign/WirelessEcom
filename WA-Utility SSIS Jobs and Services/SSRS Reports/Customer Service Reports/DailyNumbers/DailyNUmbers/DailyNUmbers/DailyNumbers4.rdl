  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>DECLARE @TotalActivated int

WITH OrdertoDetailId (OrderId, OrderDetailId) AS
(SELECT OrderId, OrderDetailId FROM salesorder.orderdetail)


SELECT @TotalActivated = COUNT(swl.wirelesslineid)
FROM OrdertoDetailId ODI
INNER JOIN salesorder.wirelessaccount swa ON swa.OrderId = ODI.OrderId
INNER JOIN salesorder.wirelessline swl ON swl.OrderDetailId = ODI.OrderDetailId
WHERE CONVERT(VARCHAR(10),swa.ActivationDate,101) = CONVERT(VARCHAR(10), DATEADD(day, -1, GETDATE()), 101)

DECLARE @ActivationsWaiting int
SELECT @ActivationsWaiting = COUNT(*) FROM salesOrder.ActivationsWaiting

DECLARE @CapturesPending int
SELECT @CapturesPending = COUNT(*) FROM salesOrder.CapturesWaiting

DECLARE @TotalHoppers int
SELECT @TotalHoppers = SUM(@ActivationsWaiting+@CapturesPending)


DECLARE @TotalCancelled int
SELECT @TotalCancelled = COUNT(sod.orderdetailid) FROM salesorder.[Order] so
INNER JOIN salesorder.[OrderDetail] sod on sod.OrderId = so.OrderId AND sod.OrderDetailType = 'd'
WHERE
OrderDate BETWEEN CONVERT(VARCHAR(25),DATEADD(dd,-(DAY(GETDATE())-1),GETDATE()),101) AND GETDATE()
AND Status = '4'

DECLARE @TotalOrders int
SELECT @TotalOrders = COUNT(sod.OrderDetailId) FROM salesorder.[Order] so
INNER JOIN salesorder.[OrderDetail] sod on sod.OrderId = so.OrderId AND sod.OrderDetailType = 'd'
WHERE
OrderDate BETWEEN CONVERT(VARCHAR(25),DATEADD(dd,-(DAY(GETDATE())-1),GETDATE()),101) AND GETDATE()
AND Status &gt; '0'

DECLARE @TotalUpgrades int
SELECT @TotalUpgrades = COUNT(sod.OrderDetailId) FROM salesorder.[Order] so
INNER JOIN salesorder.[OrderDetail] sod on sod.OrderId = so.OrderId AND sod.OrderDetailType = 'd'
WHERE
OrderDate BETWEEN CONVERT(VARCHAR(25),DATEADD(dd,-(DAY(GETDATE())-1),GETDATE()),101) AND GETDATE()
AND Status &gt; '0' AND so.ActivationType = 'U'

DECLARE @TotalNew int
SELECT @TotalNew = COUNT(sod.OrderDetailId) FROM salesorder.[Order] so
INNER JOIN salesorder.[OrderDetail] sod on sod.OrderId = so.OrderId AND sod.OrderDetailType = 'd'
WHERE
OrderDate BETWEEN CONVERT(VARCHAR(25),DATEADD(dd,-(DAY(GETDATE())-1),GETDATE()),101) AND GETDATE()
AND Status &gt; '0' AND so.ActivationType = 'N'

DECLARE @TotalAddaline int
SELECT @TotalAddaline = COUNT(sod.OrderDetailId) FROM salesorder.[Order] so
INNER JOIN salesorder.[OrderDetail] sod on sod.OrderId = so.OrderId AND sod.OrderDetailType = 'd'
WHERE
OrderDate BETWEEN CONVERT(VARCHAR(25),DATEADD(dd,-(DAY(GETDATE())-1),GETDATE()),101) AND GETDATE()
AND Status &gt; '0' AND so.ActivationType = 'A'


DECLARE @CancellationRate int
SELECT @CancellationRate = (@TotalCancelled*100)/NULLIF(@TotalOrders,0)

DECLARE @TotalReceived int
SELECT @TotalReceived = COUNT(*) FROM salesorder.[Order] WHERE CONVERT(VARCHAR(10),OrderDate,101) = CONVERT(VARCHAR(10), DATEADD(day, -1, GETDATE()), 101) AND Status &gt; '0'

DECLARE @PossibleActivations int
SELECT @PossibleActivations = COUNT(*) FROM salesorder.[OrderDetail] WHERE OrderDetailtype = 'd' AND OrderId IN (SELECT OrderId FROM salesorder.[order] WHERE CONVERT(VARCHAR(10),OrderDate,101) = CONVERT(VARCHAR(10), DATEADD(day, -1, GETDATE()), 101) AND Status &gt; '0')

DECLARE @DevicesPendingActivation int
