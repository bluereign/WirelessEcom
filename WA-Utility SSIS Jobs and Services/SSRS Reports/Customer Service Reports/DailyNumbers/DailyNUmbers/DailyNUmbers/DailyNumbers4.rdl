        <CommandText>DECLARE @SquareTradeAddCount int
DECLARE @DeviceCount int

SELECT @SquareTradeAddCount = COUNT(*) FROM salesorder.[OrderDetail] sod
WHERE sod.OrderDetailType = 'w'
AND sod.OrderId IN (SELECT OrderId FROM salesorder.[order]
WHERE Status &gt; '0' AND cast(ORderDate as date) = cast(getdate()-1 as date))

SELECT @DeviceCount = COUNT(*) FROM salesorder.[OrderDetail] sod
WHERE sod.OrderDetailType = 'd'
AND sod.OrderId IN (SELECT OrderId FROM salesorder.[order]
WHERE Status &gt; '0' AND cast(ORderDate as date) = cast(getdate()-1 as date))

DECLARE @AttachRate decimal (9,2)
SELECT @AttachRate = CONVERT(decimal (9,4),@SquareTradeAddCount)/NULLIF(CONVERT(decimal (9,4),@DeviceCount),'0')*100

SELECT @SquareTradeAddCount AS 'WarrantyAdd', @AttachRate AS 'AttachRate'
