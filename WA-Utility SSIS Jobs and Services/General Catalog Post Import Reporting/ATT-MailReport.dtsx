<DTS:Property DTS:Name="CreationName">Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=10.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91</DTS:Property>
<DTS:Property DTS:Name="DisableEventHandlers">0</DTS:Property><DTS:ObjectData><SQLTask:SqlTaskData SQLTask:Connection="{35EBFA4D-56E0-4AE7-B150-926D35AFBDE7}" SQLTask:TimeOut="1600" SQLTask:IsStoredProc="False" SQLTask:BypassPrepare="True" SQLTask:SqlStmtSourceType="DirectInput" SQLTask:SqlStatementSource="DECLARE @filename varchar(100)&#xA;&#xA;SET @filename='C:\Users\Public\Documents\PostImportCarrierReporting\CatalogLoad.xlsx' &#xA;&#xA;DECLARE @table AS TABLE(Emp_Email NVARCHAR(100), ID int IDENTITY(1,1))&#xA;&#xA;--populate the above table&#xA;INSERT @table (Emp_Email) VALUES('nhall@wirelessadvocates.com')&#xA;--INSERT @table (Emp_Email) VALUES('csmith@wirelessadvocates.com')&#xA;&#xA;DECLARE @count AS int&#xA;SET @count =1 --initialize the count parameter&#xA;DECLARE @Recepient_Email AS VARCHAR(100)&#xA;WHILE (@count &lt;=(SELECT COUNT(*) FROM @table))&#xA;        BEGIN&#xA;        SET @Recepient_Email =(SELECT TOP(1) Emp_Email FROM @table WHERE ID=@count)&#xA;&#xA;EXECUTE msdb.dbo.sp_send_dbmail&#xA;    @recipients = @Recepient_Email,&#xA;    @subject = 'Catalog Load Summary for AT&amp;T',&#xA;    @profile_name = 'Default',&#xA;    @body_format = 'html',&#xA;    @file_attachments = @filename,&#xA;    @exclude_query_output = 1,&#xA;    @append_query_error = 1,&#xA;    @query = '&#xA;    PRINT ''&lt;html&gt;''&#xA;    USE [TEST.WIRELESSADVOCATES.COM]&#xA;	PRINT ''&lt;br&gt;&lt;br&gt;''&#xA;	PRINT ''&lt;span style=&quot;font-family:Calibri,Verdana,serif&quot;&gt;''&#xA;	PRINT ''&lt;h2&gt;AT&amp;T Catalog Load on '' + CONVERT(varchar(100), GETDATE()) + ''&lt;/h2&gt;''&#xA;    PRINT ''&lt;span style=&quot;font-size:small;&quot;&gt;Attached is the catalog load job summary for the carrier AT&amp;T.&lt;/span&gt;''&#xA;    PRINT ''&lt;br&gt;&lt;br&gt;''&#xA;				DECLARE&#xA;				@querycount1 int&#xA;				,@querycount2 int&#xA;				,@querycount3 int&#xA;				,@querycount4 int&#xA;				,@querycount5 int&#xA;				,@querycount6 int&#xA;				,@querycount7 int&#xA;				,@querycount8 int&#xA;&#xA;			SELECT @querycount1 = COUNT(*) FROM [TEST.WIRELESSADVOCATES.COM].logging.catalogload_device&#xA;			SELECT @querycount2 = COUNT(*) FROM [TEST.WIRELESSADVOCATES.COM].logging.catalogload_devicerateplan&#xA;			SELECT @querycount3 = COUNT(*) FROM [TEST.WIRELESSADVOCATES.COM].logging.catalogload_deviceservice&#xA;			SELECT @querycount4 = COUNT(*) FROM [TEST.WIRELESSADVOCATES.COM].logging.catalogload_property&#xA;			SELECT @querycount5 = COUNT(*) FROM [TEST.WIRELESSADVOCATES.COM].logging.catalogload_rateplan&#xA;			SELECT @querycount6 = COUNT(*) FROM [TEST.WIRELESSADVOCATES.COM].logging.catalogload_rateplanservice&#xA;			SELECT @querycount7 = COUNT(*) FROM [TEST.WIRELESSADVOCATES.COM].logging.catalogload_service&#xA;			SET @querycount8 = SUM(@querycount1+@querycount2+@querycount3+@querycount4+@querycount5+@querycount6+@querycount7)&#xA;    PRINT ''&lt;b&gt;Catalog Changes Detected:&lt;/b&gt;'' SET NOCOUNT ON SELECT @querycount8&#xA;	PRINT ''&lt;/span&gt;&lt;/html&gt;'''&#xA;	&#xA;            SET @count = @count + 1&#xA;            END" SQLTask:CodePage="1252" SQLTask:ResultType="ResultSetType_None" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask"/></DTS:ObjectData></DTS:Executable>
